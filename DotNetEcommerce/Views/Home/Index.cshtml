<script type="text/javascript">

    $(document).ready(function () {
        var total = 0;

        $(".add-cart").click(function () {

            var buttonId = this.id;
            var slNo = buttonId.split('').at(-1);

            var priceDom = '#price-' + slNo;
            var productNameDom = '#name-' + slNo;

            var data = {};
            var price = $(priceDom).text().trim();
            var name = $(productNameDom).text().trim();
            price=price.split(' ').at(-1);
            data["Price"] = parseFloat(price);
            data["Name"] = name;

            total += parseFloat(price);
            $('#minicartTotal').text('Tk ' + total);

            $.ajax({
                url: '/Home/AddToCart',
                type: "POST",
                dataType: 'JSON',
                contentType: "application/json",
                data: JSON.stringify(data),
                success: function (data) {
                    $("#dialog").dialog({
                        buttons: {
                            Ok: function () {
                                $(this).dialog("close");
                            }
                        }
                    });
                },
                error: function (data) {

                    
                    alert(data);
                }
            });

        });

    });


</script>